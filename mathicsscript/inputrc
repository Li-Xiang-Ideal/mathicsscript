"\e$o\e": "ọ"
"\e.R\e": "Ṛ"
"\e$R\e": "Ṛ"
"\e.r\e": "ṛ"
"\e$r\e": "ṛ"
"\e.S\e": "Ṣ"
"\e$S\e": "Ṣ"
"\e.s\e": "ṣ"
"\e$s\e": "ṣ"
"\e.T\e": "Ṭ"
"\e$T\e": "Ṭ"
